        method: "get",
        success: function (data) {
            for (var i = 20; i < 28; i++) {
                $('<a href="<%=basePath%>/wine/goods.do?wid=' + data[i].wid + '" target="_blank">'
                    + ' <img src="<%=basePath%>/resource/views/img/' + data[i].wineImg1 + '" alt=""  /></a>').appendTo('.header_bottom_footer_top_content_moreActivity_one');
