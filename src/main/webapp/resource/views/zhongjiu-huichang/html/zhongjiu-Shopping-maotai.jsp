        }, 200);
    });
    $.ajax({
        url: "<%=basePath%>/wine/findLike.do?likeName=茅台",
        method: "get",
        success: function (data) {
            for (var i = 5; i < 13; i++) {
                $("<div class='min_picture'>"
                    + " <a href='<%=basePath%>/wine/goods.do?wid=" + data[i].wid + "' target='_blank'>"
                    + " <img src='<%=basePath%>/resource/views/img/" + data[i].wineImg1 + "' alt='' />"
                    + " <p class='ad-line'>" + data[i].wineScent + "<span>" + data[i].wineShiyong + "</span></p>"
                    + " <p class='name-line'>" + data[i].wineDegree + "°<span>" + data[i].wineName + "</span><span>" + data[i].wineLiter + "</span></p>"
                    + " <p class='my-price-line-zi_button-line clearfix'>"
                    + " <span class='my-price-line-zi fl'>活动价：￥<em class='my-price'><i>" + data[i].winePrice + "</i></em></span>"
                    + " <a class='button-line fr' href='###'>立即抢购</a></p></a></div>").appendTo('.header_bottom_footer_top_content_con_sw_two_middle');
